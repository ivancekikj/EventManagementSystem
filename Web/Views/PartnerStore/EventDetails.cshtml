@model Domain.Models.PartnerStoreEvent

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Partner Event</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Event Name
        </dt>
        <dd class="col-sm-10">
            @Model.EventName
        </dd>
        <dt class="col-sm-2">
            Host Name
        </dt>
        <dd class="col-sm-10">
            @Model.HostName
        </dd>
        <dt class="col-sm-2">
            Image
        </dt>
        <dd class="col-sm-10">
            <img src="@Model.ImageUrl" alt="Image of @Model.EventName" />
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Events">Back to List</a>
</div>
